{
  10u   trignl1
  10u   trigpe3
}
else
{
  10u   trigne1
}
  10u   setnmr3^6
  TrigD0
}
}
# 76 "/opt/PV5.1/exp/stan/nmr/lists/pp/mic_msme.ppg" 2

;EXEC_begin SliceSelIr line 1547 (77) SliceSelIr()
 
