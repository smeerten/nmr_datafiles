{
     50u   IrFL.res
irloop9,    10u IrFL :f1
    IrD4 grad{ (0)|(0)| PVM_SelIrGradAmp(100,,no_scale)}
    IrD3 gatepulse 1
    IrP0:sp2  ph0
    IrD4 groff
    IrD0 islice IrFL.inc
    lo to irloop9 times IrL0
    IrD2  grad{ (0)|(0)| PVM_SelIrSpoiler(100,,no_scale)}
    IrD4  groff IrFL.res
    IrD1 zslice
}

 
;EXEC_end   SliceSelIr line 1547 (77) SliceSelIr()
slice,  10u     fq8b:f1

